<%= render partial:'/shared/nav_page', locals: {title: 'New Category', backurl:
user_categories_path(user_id:@user.id)} %>
<div class="custom-category-page-header">
  <div class="custom-category-icon">
    <i class="material-icons outlined md-48"><%= @category.icon%></i>
  </div>
  <h2>Category: <%= @category.name%></h2>
</div>

<div class="custom-container">
  <div class="">
    <% @spendings.each do |spending| %>
    <div class="spending-card">
      <h3>
        <%= spending.name %>
      </h3>
      <h5> Amount: 
        <%= spending.name %>
      </h5>
    </div>
    <% end %>
  </div>
</div>
<%= link_to "New Spending" ,new_user_category_spending_path(user_id:@user.id,category_id:@category.id) %>
